        "sass": "1.32.8",
        "sass-loader": "11.0.1",
        "source-map-explorer": "2.5.2",
        "style-loader": "2.0.0",
        "ts-node": "10.0.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
