        "@reduxjs/toolkit": "1.5.0",
        "axios": "0.21.1",
        "customize-cra": "1.0.0",
        "eslint-plugin-standard": "5.0.0",
        "moment": "2.29.4",
        "moment-timezone": "0.5.34",
        "postcss": "^8.2.6",
        "react": "^17.0.2",
        "react-device-detect": "2.2.2",
        "react-dnd": "14.0.2",
        "react-dnd-html5-backend": "14.0.0",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
