        "lint-fix": "eslint --fix \"src/**/*.+(js|jsx|ts|tsx|json)\"",
        "prettier": "prettier --write \"src/**/*.+(js|jsx|ts|tsx|json)\"",
        "build": "REACT_APP_IS_DEV=dev react-scripts build"
